{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Protocol Data",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "time": {
          "type": "integer",
          "minimum": 0,
          "maximum": 9007199254740991
          
        },
        "new": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "type": { "type": "string" },
              "properties": {
                "type": "object",
                "additionalProperties": { "type": "string" }
              }
            },
            "required": ["id", "type", "properties"]
          }
        },
        "update": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "type": { "type": "string" },
              "properties": {
                "type": "object",
                "additionalProperties": { "type": "string" }
              }
            },
            "required": ["id", "type", "properties"]
          }
        },
        "delete": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "type": { "type": "string" }
            },
            "required": ["id", "type"]
          }
        }
      },
      "required": ["time"],
      "anyOf": [
        { "required": ["new"] },
        { "required": ["update"] },
        { "required": ["delete"] }
      ]
    }
  }
  