{
    "title": "Multiservicios Aeroportuarios Problem",
    "description": "Description of instances of the Multiservicios Aeroportuarios Problem",
    "type": "object",
    "properties": {
		"travelTime": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"source": {
						"type": "integer"
					},
					"time": {
						"type": "integer",
						"minimum": 1,
                        "maximum": 100
					},
					"target": {
						"type": "integer"
					}
				},
				"required": [
					"source",
					"time",
					"target"
				]
			}
		},
        "flights": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string",
                        "minLength": 1,
                        "maxLength": 100
                    },
                    "employees": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 100
                    },
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "finishTime": {
                        "type": "string",
                        "format": "date-time"
                    },
					"serviceTime": {
						"type": "string",
                        "format": "duration"
                    },
                },
                "required": [
                    "code",
                    "employees",                    
                    "startTime",
                    "finishTime",
					"serviceTime"
                ]
            }
        },
        "employees": {
            "type": "object",
            "properties": {
                "individuals": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "additionalProperties": true,
                        "properties": {
                            "code": {
                                "type": "string",
                                "minLength": 1,
                                "maxLength": 10
                            },
                            "timePerDay": {
                                "type": "string",
                                "format": "duration"
                            },
                            "timePerWeek": {
                                "type": "string",
                                "format": "duration"
                            },
							"notAvailableDates": {
								"type": "array",
								"items": {
									"type": "string",
									"format": "date"
								}
							},
                            "timeBetweenWorkingDays": {
                                "type": "string",
                                "format": "duration"
                            },
							"startTime": {
								"type": "array",
								"items": {
									"type": "object",
									"properties": {
										"dates": {
											"type": "array",
											"items": {
												"type": "string",
												"format": "date"
											}
										},
										"time": {
											"type": "string",
											"format": "time"
										}
									},
									"required": [
										"dates",
										"time"
									]
								}
							},
							"finishTime": {
								"type": "array",
								"items": {
									"type": "object",
									"properties": {
										"dates": {
											"type": "array",
											"items": {
												"type": "string",
												"format": "date"
											}
										},
										"time": {
											"type": "string",
											"format": "time"
										}
									},
									"required": [
										"dates",
										"time"
									]
								}
							},
                            "enterTime": {
                                "type": "string",
                                "format": "duration"
                            },
                            "breakTime": {
                                "type": "string",
                                "format": "duration"
                            },
                            "leaveTime": {
                                "type": "string",
                                "format": "duration"
                            },
                            "notAvailableDates": {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "date"
                                }
                            }
                        },
                        "required": [
                            "code",                           
                            "timePerDay",
                            "timePerWeek"                        
                        ]
                    }
                },
                "parameters": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "enterTime": {
                            "type": "string",
                            "format": "duration"
                        },
                        "breakTime": {
                            "type": "string",
                            "format": "duration"
                        },
                        "leaveTime": {
                            "type": "string",
                            "format": "duration"
                        },
                        "timeBetweenWorkingDays": {
                            "type": "string",
                            "format": "duration"
                        }
                    },
                    "required": [
                        "enterTime",
                        "breakTime",
                        "leaveTime",
                        "timeBetweenWorkingDays"                        
                    ]
                }
            }
        },
        "airport": {
            "type": "string",
            "minLength": 3,
            "maxLength": 3,
            "pattern": "^[A-Z]{3}$"
        }, 
        "group": {
            "type": "string",
            "minLength": 2,
            "maxLength": 20
        }
    },
    "required": [
        "flights",
		"travelTime"
    ]
}
